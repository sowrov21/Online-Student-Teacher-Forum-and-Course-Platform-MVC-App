@model List<OnlineStudentTeacherCollabAppp.Models.User>

@{
    ViewBag.Title = "ShowProfile";
}

<h2>ShowProfile</h2>
<table class="table table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Type</th>
        <th>Address</th>
        <th>Password</th>
    </tr>
    
    @foreach (var item in Model)
    {
        
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Type</td>
                <td>@item.Address</td>
                <td>@item.Password</td>
                <td>
                    <a href="@Url.Action("Update", new { item.Id, item.Name,item.Email,item.Type,item.Address,item.Password})" class="btn btn-success"> Edit </a>
                </td>
            </tr>
        }
</table>

